{
  "name": "gsx-power-user",
  "productName": "Onereach.ai",
  "version": "4.0.0",
  "description": "GSX Power User Desktop Application",
  "main": "main.js",
  "icon": "assets/icon.icns",
  "scripts": {
    "postinstall": "node scripts/download-claude-code.js",
    "start": "electron .",
    "dev": "NODE_ENV=development npx electron .",
    "dev:win": "set NODE_ENV=development && npx electron .",
    "package": "npx electron-builder build --mac --win --publish never",
    "package:mac": "npx electron-builder build --mac --publish never",
    "package:win": "npx electron-builder build --win --publish never",
    "package:mac:arm64": "npx electron-builder build --mac arm64 --publish never",
    "package:mac:x64": "npx electron-builder build --mac x64 --publish never",
    "package:win:arm64": "npx electron-builder build --win arm64 --publish never",
    "package:win:x64": "npx electron-builder build --win x64 --publish never",
    "package:universal": "npx electron-builder build --mac universal --win arm64,x64 --publish never",
    "package:mac:universal": "npx electron-builder build --mac --universal --publish never",
    "publish": "npx electron-builder build --mac --win --publish always",
    "publish:mac": "npx electron-builder build --mac --publish always",
    "publish:win": "npx electron-builder build --win --publish always",
    "publish:github": "npx electron-builder build --mac --win --publish always --publish-config publish.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --exclude='**/*.eval.*'",
    "test:evals": "vitest run --include='**/*.eval.*'",
    "test:e2e": "playwright test",
    "test:e2e:products": "playwright test test/e2e/products/",
    "test:e2e:quick": "playwright test test/e2e/window-smoke.spec.js test/e2e/api-integration.spec.js",
    "test:smoke": "playwright test test/e2e/window-smoke.spec.js",
    "test:orb": "playwright test test/e2e/voice-orb.spec.js",
    "test:orb:corpus": "playwright test test/e2e/voice-orb-corpus.spec.js",
    "test:orb:all": "playwright test test/e2e/voice-orb.spec.js test/e2e/voice-orb-corpus.spec.js",
    "test:api": "playwright test test/e2e/api-integration.spec.js",
    "test:journey": "playwright test test/e2e/window-smoke.spec.js test/e2e/voice-orb.spec.js test/e2e/voice-orb-corpus.spec.js test/e2e/api-integration.spec.js test/e2e/spaces-flow.spec.js test/e2e/settings-flow.spec.js",
    "test:full": "npm run test:unit && playwright test test/e2e/products/ test/e2e/window-smoke.spec.js test/e2e/api-integration.spec.js test/e2e/spaces-flow.spec.js",
    "test:e2e:ai-conversation": "TEST_MODE=true playwright test test/e2e/ai-conversation-capture.spec.js",
    "test:e2e:ai-conversation:headed": "TEST_MODE=true playwright test test/e2e/ai-conversation-capture.spec.js --headed",
    "test:e2e:ai-conversation:debug": "TEST_MODE=true PWDEBUG=1 playwright test test/e2e/ai-conversation-capture.spec.js",
    "test:e2e:ai-conversation:smoke": "TEST_MODE=true playwright test test/e2e/ai-conversation-smoke.spec.js",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm run test && npm run test:e2e",
    "test:bidding": "vitest run test/unit/distributed-bidding.test.js",
    "test:bidding:watch": "vitest watch test/unit/distributed-bidding.test.js",
    "test:corpus": "node test/corpus/bidding-test-corpus.js",
    "test:corpus:llm": "node test/corpus/llm-bidding-test-corpus.js",
    "test:wizard": "electron test/setup-wizard-test-suite.js",
    "test:external-ai": "electron test/external-ai-test-suite.js",
    "test:external-ai-interactive": "electron test/external-ai-interactive-test.js",
    "test:idw": "electron test/run-idw-tests.js",
    "test:gsx-sync": "node test/test-gsx-sync.js",
    "test:blackhole": "node test-black-hole.js",
    "test:metadata": "node test/test-metadata-generation.js",
    "test:metadata-api": "node test/test-metadata-api-integration.js",
    "test:converters": "vitest run test/unit/converters/",
    "test:conversion": "vitest run test/unit/converters/ test/unit/base-converter-agent.test.js test/unit/conversion-service.test.js test/unit/pipeline-resolver.test.js test/unit/playbook-validator.test.js test/unit/playbook-diagnostics.test.js",
    "test:conversion:evals": "vitest run test/evals/converter-quality.eval.js test/evals/playbook-quality.eval.js",
    "test:conversion:all": "npm run test:conversion && npm run test:conversion:evals",
    "test:generate-fixtures": "node test/fixtures/conversion/generate-fixtures.js && node test/fixtures/media/generate-media-fixtures.js",
    "test:audit": "node test/audit/cli.js",
    "test:audit:status": "node test/audit/cli.js status",
    "test:audit:next": "node test/audit/cli.js next",
    "test:audit:failed": "node test/audit/cli.js failed",
    "test:audit:retry": "node test/audit/cli.js retry-failed",
    "test:audit:restart": "node test/audit/cli.js restart",
    "test:audit:regression": "node test/audit/cli.js regression",
    "test:audit:report": "node test/audit/cli.js report",
    "test:fixtures:generate": "node test/fixtures/conversion/generate-fixtures.js",
    "dist": "electron-builder",
    "download-claude": "node scripts/download-claude-code.js",
    "download-claude:all": "node scripts/download-claude-code.js --all",
    "build:signed": "./scripts/build-signed.sh",
    "build:signed:skip-notarize": "./scripts/build-signed.sh --skip-notarize",
    "release": "./scripts/release-master.sh",
    "release:quick": "./scripts/release-master.sh --quick"
  },
  "build": {
    "appId": "com.gsx.poweruser",
    "productName": "Onereach.ai",
    "asarUnpack": [
      "aider_bridge/**/*"
    ],
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/.{idea,git,github,vscode,cursor}/**",
      "!**/{.DS_Store,.git,.gitignore,.npmignore}",
      "!test/**/*",
      "!test-black-hole*.js",
      "!test-api-*.js",
      "!test-electron-*.js",
      "!test-elevenlabs-*.js",
      "!test-idw-*.js",
      "!test-multimodal-*.js",
      "!test-onereach-*.js",
      "!test-sync-*.js",
      "!test-token-*.js",
      "!test-local-*.sh",
      "!*.sh",
      "!api-*.json",
      "!example-module/**/*",
      "!test-update-server/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "assets/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "notarize": true
    },
    "afterSign": "scripts/notarize.js",
    "win": {
      "icon": "assets/tray-icon.png",
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "icon": "assets/tray-icon.png",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "dmg": {
      "background": "assets/dmg/background.png",
      "icon": "assets/icon.icns",
      "iconSize": 128,
      "contents": [
        {
          "x": 130,
          "y": 150
        },
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "directories": {
      "output": "dist"
    },
    "extraResources": [
      {
        "from": "resources/claude-code/darwin-arm64",
        "to": "claude-code/darwin-arm64",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "resources/claude-code/darwin-x64",
        "to": "claude-code/darwin-x64",
        "filter": [
          "**/*"
        ]
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "wilsr7000",
        "repo": "Onereach_Desktop_App",
        "releaseType": "release"
      }
    ]
  },
  "keywords": [
    "electron",
    "onereach",
    "ai"
  ],
  "author": "Onereach.ai",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/wilsr7000/onereach_desktop.git"
  },
  "devDependencies": {
    "@electron/notarize": "^3.0.1",
    "@playwright/test": "^1.57.0",
    "@vitest/coverage-v8": "^4.0.16",
    "adm-zip": "^0.5.16",
    "electron": "39.2.7",
    "electron-builder": "^24.9.1",
    "husky": "^9.1.7",
    "promptfoo": "^0.120.11",
    "vitest": "^4.0.16",
    "vitest-evals": "^0.5.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@duckdb/node-api": "^1.4.2-r.1",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@or-sdk/files-sync-node": "^0.0.4",
    "7zip-bin": "^5.2.0",
    "archiver": "^7.0.1",
    "assemblyai": "^4.23.0",
    "docx": "^9.5.1",
    "electron-audio-loopback": "^1.0.6",
    "electron-log": "^5.3.3",
    "electron-updater": "^6.6.2",
    "exceljs": "^4.4.0",
    "fast-xml-parser": "^5.3.5",
    "fluent-ffmpeg": "^2.1.3",
    "highlight.js": "^11.11.1",
    "isomorphic-git": "^1.36.3",
    "js-yaml": "^4.1.1",
    "jsqr": "^1.4.0",
    "keytar": "^7.9.0",
    "mammoth": "^1.11.0",
    "marked": "^17.0.1",
    "node-pty": "^1.1.0",
    "otplib": "^13.2.1",
    "pdf-parse": "^2.4.5",
    "playwright": "^1.40.1",
    "podcast-api": "^2.0.4",
    "pptxgenjs": "^4.0.1",
    "puppeteer": "^24.31.0",
    "sharp": "^0.34.5",
    "turndown": "^7.2.2",
    "unzipper": "^0.10.14",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "youtube-dl-exec": "^3.0.10"
  }
}
